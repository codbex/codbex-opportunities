            "select": true,
            "grouping": false
        }
    ],
    "query": "SELECT Opportunity.OPPORTUNITY_ID as \"Id\", Customer.CUSTOMER_NAME as \"Name\", OpportunityNote.OPPORTUNITYNOTE_TIMESTAMP as \"Date\", OpportunityNote.OPPORTUNITYNOTE_DESCRIPTION as \"Description\", ActionType.ACTIONTYPE_NAME as \"Type\", ActionStatus.ACTIONSTATUS_NAME as \"Status\"\nFROM CODBEX_OPPORTUNITY as Opportunity\n  INNER JOIN CODBEX_CUSTOMER Customer ON Opportunity.OPPORTUNITY_CUSTOMER = Customer.CUSTOMER_ID\n  INNER JOIN CODBEX_OPPORTUNITYACTION OpportunityAction ON Opportunity.OPPORTUNITY_ID = OpportunityAction.OPPORTUNITYACTION_OPPORTUNITY\n  INNER JOIN CODBEX_ACTIONTYPE ActionType ON OpportunityAction.OPPORTUNITYACTION_TYPE = ActionType.ACTIONTYPE_ID\n  INNER JOIN CODBEX_ACTIONSTATUS ActionStatus ON OpportunityAction.OPPORTUNITYACTION_STATUS = ActionStatus.ACTIONSTATUS_ID\n  INNER JOIN CODBEX_OPPORTUNITYNOTE OpportunityNote ON OpportunityAction.OPPORTUNITYACTION_ID = OpportunityNote.OPPORTUNITYNOTE_ACTION\nORDER BY OPPORTUNITYACTION_DATE DESC",
    "joins": [
        {
            "alias": "Customer",
            "name": "CODBEX_CUSTOMER",
            "type": "INNER",
            "condition": "Opportunity.OPPORTUNITY_CUSTOMER = Customer.CUSTOMER_ID"
        },
        {
            "alias": "OpportunityAction",
            "name": "CODBEX_OPPORTUNITYACTION",
            "type": "INNER",
            "condition": "Opportunity.OPPORTUNITY_ID = OpportunityAction.OPPORTUNITYACTION_OPPORTUNITY"
        },
        {
            "alias": "ActionType",
            "name": "CODBEX_ACTIONTYPE",
            "type": "INNER",
            "condition": "OpportunityAction.OPPORTUNITYACTION_TYPE = ActionType.ACTIONTYPE_ID"
        },
        {
            "alias": "ActionStatus",
            "name": "CODBEX_ACTIONSTATUS",
            "type": "INNER",
            "condition": "OpportunityAction.OPPORTUNITYACTION_STATUS = ActionStatus.ACTIONSTATUS_ID"
        },
        {
            "alias": "OpportunityNote",
            "name": "CODBEX_OPPORTUNITYNOTE",
            "type": "INNER",
            "condition": "OpportunityAction.OPPORTUNITYACTION_ID = OpportunityNote.OPPORTUNITYNOTE_ACTION"
        }
    ],
    "orderings": [
        {
            "column": "OPPORTUNITYACTION_DATE",
            "direction": "DESC"
        }
    ]
}